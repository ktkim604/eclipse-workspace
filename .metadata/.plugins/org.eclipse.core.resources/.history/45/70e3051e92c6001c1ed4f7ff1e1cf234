
public class Example {
	
		int a ;
		
		boolean odd(int b){
			if(b %2!=0) {
				return true;
				
			}
			else {
				return false;
			}
		}
		int min(int arr[]) {
			int min = 100;
			for (int i =0;i<arr.length;i++) {
				if(min>arr[i]) {
					min = arr[i];
				}
				
			}
			return min;
		}
		
		
	}


